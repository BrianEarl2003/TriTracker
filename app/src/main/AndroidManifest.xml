<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.brianearlseniorproject">


    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/tritracker"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tritracker_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BrianEarlSeniorProject">
        <activity
            android:name=".RunGoalActivity"
            android:exported="false" />
        <activity
            android:name=".BikeGoalActivity"
            android:exported="false" />
        <activity
            android:name=".SwimGoalActivity"
            android:exported="false" />
        <activity
            android:name=".RunActivity"
            android:exported="false" />
        <activity
            android:name=".BikeActivity"
            android:exported="false" />
        <activity
            android:name=".SwimActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>